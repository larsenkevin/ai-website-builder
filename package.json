{
  "name": "ai-website-builder",
  "version": "1.0.0",
  "description": "AI-powered website builder for small business owners",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch app/server.ts",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\""
  },
  "keywords": [
    "website-builder",
    "ai",
    "claude",
    "cms"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "sharp": "^0.33.5",
    "chokidar": "^4.0.3",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/cors": "^2.8.17",
    "@types/node": "^22.10.5",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.7.3",
    "tsx": "^4.19.2",
    "vitest": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "fast-check": "^3.23.2",
    "@eslint/js": "^9.18.0",
    "eslint": "^9.18.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
